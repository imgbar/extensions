{
  "manifest_version": 3,
  "name": "Imgbar",
  "version": "0.0.1",
  "description": "Edit images quickly",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/favicon_48.png",
    "180": "icons/icon_180.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "src/background.js"
  }
}